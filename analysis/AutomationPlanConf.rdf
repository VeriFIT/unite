<?xml version="1.0" encoding="UTF-8"?>
<rdf:RDF
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:dcterms="http://purl.org/dc/terms/"
    xmlns:fit="http://verifit/fit#"
    xmlns:oslc_data="http://open-services.net/ns/servicemanagement/1.0/"
    xmlns:oslc="http://open-services.net/ns/core#"
    xmlns:foaf="http://xmlns.com/foaf/0.1/#"
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
    xmlns:oslc_auto="http://open-services.net/ns/auto#">

    <oslc_auto:AutomationPlan>

        <dcterms:identifier>anaconda</dcterms:identifier>
        <oslc_auto:usesExecutionEnvironment rdf:resource="/home/od42/matrix/proj_praxe/anaconda/tools/run.sh"/>

        <dcterms:title rdf:parseType="Literal">ANaConDA</dcterms:title>
        <dcterms:creator rdf:resource="https://pajda.fit.vutbr.cz/xvasic"/>
        <dcterms:description rdf:parseType="Literal">Analyse an SUT using ANaConDA</dcterms:description> 

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>analyser</oslc:name>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Exactly-one"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">Specify what analyser to use</dcterms:description>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>
        
        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>launchSUT</oslc:name>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <dcterms:description rdf:parseType="Literal">This parameter definitions tells the Automation Plan to place the SUT launch command at this command line position</dcterms:description>
                <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</oslc:readOnly>
                <oslc:defaultValue>2</oslc:defaultValue>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">3</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">Set the execution parameters for the analyzed program. Write down all parameters as you would in a console.</dcterms:description>
                <oslc:defaultValue></oslc:defaultValue>
                <oslc:name>executionParameters</oslc:name>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

    </oslc_auto:AutomationPlan>


    <oslc_auto:AutomationPlan>
        <dcterms:identifier>perun</dcterms:identifier>
        <oslc_auto:usesExecutionEnvironment rdf:resource="/usr/local/bin/perun"/>
        <dcterms:title rdf:parseType="Literal">Perun</dcterms:title>
        <dcterms:creator rdf:resource="https://pajda.fit.vutbr.cz/xvasic"/>
        <dcterms:description rdf:parseType="Literal">Analyse an SUT using Perun</dcterms:description>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Exactly-one"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">2</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">Specify a perun command to run</dcterms:description>
                <oslc:name>command</oslc:name>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">Parameters needed by the adapter in order for the tools output to be xml readable.</dcterms:description>
                <oslc:defaultValue>-nc --no-pager</oslc:defaultValue>
                <oslc:name>adapterSpecific</oslc:name>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

    </oslc_auto:AutomationPlan>




    <oslc_auto:AutomationPlan>
        <dcterms:identifier>hilite</dcterms:identifier>
        <oslc_auto:usesExecutionEnvironment rdf:resource="/home/od42/matrix/proj_praxe/dummy_hilite.sh"/>
        <dcterms:title rdf:parseType="Literal">Hilite</dcterms:title>
        <dcterms:creator rdf:resource="https://pajda.fit.vutbr.cz/xvasic"/>
        <dcterms:description rdf:parseType="Literal">Analyse an SUT using Hilite</dcterms:description>
        
        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>launchSUT</oslc:name>
                <oslc:defaultValue>1</oslc:defaultValue>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <dcterms:description rdf:parseType="Literal">This parameter definitions tells the Automation Provider to place the SUT launch command at this command line position</dcterms:description>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>arguments</oslc:name>
                <oslc:defaultValue></oslc:defaultValue>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">2</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">Specify which arguments should be passed to Hilite on the command line.</dcterms:description>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <dcterms:description rdf:parseType="Literal">This parameter definitions tells the Automation Provider to only allow one Automation Request at a time to execute this Automation Plan. Requests created while an instance is already running will be placed in a queue.</dcterms:description>
                <oslc:defaultValue>True</oslc:defaultValue>
                <oslc:name>oneInstanceOnly</oslc:name>
                <oslc:readOnly rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</oslc:readOnly>
                <oslc:hidden rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</oslc:hidden>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

    </oslc_auto:AutomationPlan>



    <oslc_auto:AutomationPlan>
        <dcterms:identifier>valgrind</dcterms:identifier>
        <oslc_auto:usesExecutionEnvironment rdf:resource="/usr/bin/valgrind"/>
        <dcterms:title rdf:parseType="Literal">Valgrind</dcterms:title>
        <dcterms:creator rdf:resource="https://pajda.fit.vutbr.cz/xvasic"/>
        <dcterms:description rdf:parseType="Literal">Analyse an SUT using Valgrind</dcterms:description>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>arguments</oslc:name>
                <oslc:defaultValue></oslc:defaultValue>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">Specify which arguments should be passed to Valgrind on the command line.</dcterms:description>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>
        
        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>launchSUT</oslc:name>
                <oslc:defaultValue>2</oslc:defaultValue>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <dcterms:description rdf:parseType="Literal">This parameter definitions tells the Automation Provider to place the SUT launch command at this command line position</dcterms:description>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">3</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">Set the execution parameters for the analyzed program. Write down all parameters as you would in a console.</dcterms:description>
                <oslc:defaultValue></oslc:defaultValue>
                <oslc:name>executionParameters</oslc:name>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>
    </oslc_auto:AutomationPlan>



    <oslc_auto:AutomationPlan>
        <dcterms:identifier>grep</dcterms:identifier>
        <oslc_auto:usesExecutionEnvironment rdf:resource="/bin/grep"/>
        <dcterms:title rdf:parseType="Literal">grep</dcterms:title>
        <dcterms:creator rdf:resource="https://pajda.fit.vutbr.cz/xvasic"/>
        <dcterms:description rdf:parseType="Literal">Run grep</dcterms:description>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>options</oslc:name>
                <oslc:defaultValue></oslc:defaultValue>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">1</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">grep options</dcterms:description>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>pattern</oslc:name>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Exactly-one"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">2</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">grep pattern</dcterms:description>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

        <oslc_auto:parameterDefinition>
            <oslc_auto:ParameterDefinition>
                <oslc:name>file</oslc:name>
                <oslc:occurs rdf:resource="http://open-services.net/ns/core#Zero-or-One"/>
                <fit:commandlinePosition rdf:datatype="http://www.w3.org/2001/XMLSchema#int">3</fit:commandlinePosition>
                <oslc:valueType rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
                <dcterms:description rdf:parseType="Literal">grep file</dcterms:description>
                <oslc:defaultValue></oslc:defaultValue>
            </oslc_auto:ParameterDefinition>
        </oslc_auto:parameterDefinition>

    </oslc_auto:AutomationPlan>

</rdf:RDF>