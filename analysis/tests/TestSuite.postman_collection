													"        } else { // timeout, reset counter",
													"            waitCounter = 0;",
													"        }",
													"    } else { // wait doen, reset counter",
													"            waitCounter = 0;",
													"    }",
													"}",
													"",
													"pm.globals.set(\"waitCounter\", waitCounter);"
												],
